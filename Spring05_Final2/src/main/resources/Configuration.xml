<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  <configuration>
    <!-- 자주 사용하는 type의 별칭을 등록하고 Mapper xml에서 사용할 수 있다. -->
  	<typeAliases>
  		<typeAlias type="com.gura.spring05.users.dto.UsersDto" alias="UsersDto"/>
  		<typeAlias type="com.gura.spring05.file.dto.FileDto" alias="fileDto"/>
  		<typeAlias type="com.gura.spring05.cafe.dto.CafeDto" alias="cafeDto"/>
  		<typeAlias type="com.gura.spring05.cafe.dto.CafeCommentDto" alias="cafeCommentDto"/>
  		<typeAlias type="com.gura.spring05.gallery.dto.GalleryDto" alias="galleryDto"/>
  	</typeAliases>
  	<mappers>
  	 	<mapper resource="mapper/UsersMapper.xml"/>
  	 	<mapper resource="mapper/FileMapper.xml"/>
  	 	<mapper resource="mapper/CafeMapper.xml"/>
  	 	<mapper resource="mapper/CafeCommentMapper.xml"/>
  	 	<mapper resource="mapper/GalleryMapper.xml"/>
  	</mappers>
  </configuration>